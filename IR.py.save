import RPi.GPIO as GPIO
from time import sleep
GPIO.setmode(GPIO.BCM)
GPIO.setup(18, GPIO.IN)
GPIO.setup(23, GPIO.IN)


while True:
	leftsensor = GPIO.input(18)
	if leftsensor == 0:
		print("obstacle on left")
		sleep(0.5)
	elif leftsensor == 1:
		print("no obstacle left")
		sleep(0.5)
	rightsensor = GPIO.input(23)
	if rightsensor == 0:
		print("obstacle on right")
		sleep(0.5)
	elif rightsensor == 1:
		print("no obstace right")
		sleep(0.5)

